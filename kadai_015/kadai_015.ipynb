      "source": [
        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(\"名前:\", self.name)\n",
        "        print(\"年齢:\", self.age)\n",
        "\n",
        "\n",
        "# Humanクラスのインスタンスを生成して変数に代入\n",
        "human = Human(\"貴志\", 25)\n",
        "\n",
        "# メソッドを呼び出して標準出力\n",
        "human.printinfo()\n"
      ],
